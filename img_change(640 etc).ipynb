   "source": [
    "import glob\n",
    "import os\n",
    "from PIL import Image\n",
    "files = glob.glob('C:/Users/rk401/Desktop/ㅁ둠/*.jpg')\n",
    "\n",
    "for f in files:\n",
    "    title, ext = os.path.splitext(f)\n",
    "    if ext in ['.jpg']:\n",
    "        img = Image.open(f)\n",
    "        img_resize = img.resize((640, 640))\n",
    "        img_resize.save(title+ext)"
   ]
